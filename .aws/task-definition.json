{
  "family": "springboot-docker-demo-containers",
  "containerDefinitions": [
    {
      "name": "springboot-docker-demo",
      "image": "540191367032.dkr.ecr.us-east-1.amazonaws.com/springboot-docker-demo:latest",
      "cpu": 1,
      "memory": 300,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "stage"
        }
      ]
    }
  ]
}